<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivityDashboard">

    <TextView
        android:id="@+id/textPassedEmail"
        android:layout_width="233dp"
        android:layout_height="17dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="113dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="41dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/buttonWTClass"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonWTClass"
        android:layout_width="179dp"
        android:layout_height="39dp"
        android:layout_marginStart="116dp"
        android:layout_marginEnd="116dp"
        android:text="wtclass"
        app:layout_constraintBottom_toTopOf="@+id/buttonNotes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textPassedEmail" />

    <Button
        android:id="@+id/buttonNotes"
        android:layout_width="179dp"
        android:layout_height="39dp"
        android:layout_marginStart="116dp"
        android:layout_marginEnd="116dp"
        android:text="notes"
        app:layout_constraintBottom_toTopOf="@+id/buttonWeather"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonWTClass" />

    <Button
        android:id="@+id/buttonWeather"
        android:layout_width="179dp"
        android:layout_height="39dp"
        android:layout_marginStart="116dp"
        android:layout_marginEnd="116dp"
        android:text="current weather"
        app:layout_constraintBottom_toTopOf="@+id/signoutCheck"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonNotes" />

    <Button
        android:id="@+id/signoutCheck"
        android:layout_width="179dp"
        android:layout_height="39dp"
        android:layout_marginStart="116dp"
        android:layout_marginEnd="116dp"
        android:layout_marginBottom="232dp"
        android:text="sign out"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonWeather" />

</androidx.constraintlayout.widget.ConstraintLayout>